# WordPress Theme .gitignore

# Ignore system files
.DS_Store
Thumbs.db
desktop.ini

# Ignore editor files
*.sublime-project
*.sublime-workspace
.idea/
.vscode/
*.swp
*.swo

# Ignore log files and databases
*.log
*.sql
*.sqlite

# Ignore compiled files
*.com
*.class
*.dll
*.exe
*.o
*.so

# Ignore packaged files
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip

# Ignore node dependency directories
node_modules/

# Ignore composer dependencies
vendor/

# Ignore cache directories
cache/

# Ignore environment specific files
.env
.env.*
!.env.example

# Ignore WordPress core files (if applicable)
wp-admin/
wp-includes/
wp-config.php

# Ignore uploads directory (if applicable)
wp-content/uploads/

# Ignore backup files
*~
*.bak
